public class returning{
	public static void main(String[] args){
		int i=2;int j=3;
		int result = (i+j)+(i*j);
		System.out.println("The answer :"+result);
		add();
	
	}
	
	public static int add(){
		int i=4; int j=5;
		 int res = i+j;
		System.out.println("answer:"+res);
		return res;
		
	}
	
	
	
}
