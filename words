import java.util.Scanner;

public class word{
	public static void main(String[] args){
		
		Scanner s = new Scanner(System.in);
		
		String a = "Hai";
		System.out.println("Enter a number");
		int i =s.nextInt();
		for(int j=0; j <i;j++){
			System.out.println(a);
		}
		words();
		
	}
	
	public static void words(){
Scanner s = new Scanner(System.in);
		
		String a = "Hai";
		System.out.println("Enter a number");
		int i =s.nextInt();
		int j=0;
		while(j<i)
		{
			System.out.println(a);
			j++;
		}
	}
}
